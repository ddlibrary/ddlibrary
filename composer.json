{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.0",
        "anhskohbo/no-captcha": "^3.0",
        "aws/aws-sdk-php": "^3.190",
        "barryvdh/laravel-translation-manager": "^0.5.8",
        "facebook/graph-sdk": "~5.0",
        "fideloper/proxy": "^4.3",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^8.0",
        "laravel/helpers": "^1.4",
        "laravel/socialite": "^5.2",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "mcamara/laravel-localization": "^1.6",
        "setasign/fpdf": "^1.8",
        "setasign/fpdi": "^2.3",
        "spatie/laravel-activitylog": "^4.1",
        "spatie/laravel-analytics": "^4.0",
        "spatie/laravel-google-fonts": "^1.0",
        "spatie/laravel-honeypot": "^4.0",
        "unisharp/laravel-filemanager": "~2.2",
        "usmanhalalit/laracsv": "^2.1",
        "yajra/laravel-datatables-oracle": "9.*"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.10",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "files": [
            "app/Support/Helpers.php",
            "app/Support/DrupalPasswordHasher.php"
        ],
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.local', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
